{
  "name": "cmdLog",
  "displayName": "cmdLog",
  "description": "This is AI powred log generator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cmdLog.addLog",
        "title": "Generate Log"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "logMessagePrefix": {
          "type": "string",
          "default": "cmdLogger ",
          "description": "The prefix of the log message"
        },
        "logMessageSuffix": {
          "type": "string",
          "default": ":",
          "description": "The suffix of the log message"
        },
        "logFunction": {
          "type": "string",
          "default": "",
          "description": "Custom log function to use in the inserted log message"
        },
        "addSemicolonInTheEnd": {
          "type": "boolean",
          "default": true,
          "description": "Whether to put a semicolon at the end of the log message or not"
        }
      }
    },
    "keybindings": [
      {
        "command": "cmdLog.addLog",
        "key": "ctrl+alt+l",
        "when": "editorTextFocus"
      },
      {
        "command": "cmdLog.addLog",
        "key": "cmd+alt+l",
        "when": "editorTextFocus",
        "mac": "cmd+alt+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "watch": "webpack --watch",
    "compile": "webpack",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}
